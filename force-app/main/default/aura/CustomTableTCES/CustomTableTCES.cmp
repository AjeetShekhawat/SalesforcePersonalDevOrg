<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" controller="DynamicDataTable" >
    
    
    <aura:attribute name="InputFieldName" type="String" description="Will vxfv fdvfd" default="Ajay"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    
    <div class="slds-page-header">
        <div class="slds-page-header__row">
            <div class="slds-page-header__col-title">
                <div class="slds-media">
                    <div class="slds-media__figure">
                        <span class="slds-icon_container slds-icon-standard-opportunity" title="opportunity">
                            <img src="https://sg-glass--distribute--c.cs57.visual.force.com/resource/1542953937000/SaintGobainPNGlogo?" class="logo"/>
                        </span>
                    </div>
                    <div class="slds-media__body">
                        <div class="slds-page-header__name">
                            <div class="slds-page-header__name-title">
                                <h1>
                                    <span class="slds-page-header__title slds-truncate page_title" title="Project Management">Project Management</span>
                                </h1>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    <!-- Progress Bar for completion of project -->
    <lightning:progressBar value="50" size="large" variant="circular" />
    
    
    <lightning:accordion aura:id="accordion" activeSectionName="Project Delivery 2">
        <lightning:accordionSection name="Project Delivery 1" label="Project Delivery">
            
            <table class="slds-table slds-no-row-hover slds-table_bordered slds-table_striped slds-table_col-bordered slds-table_resizable-cols slds-max-medium-table_stacked-horizontal">
                
                <thead>
                    <tr class="slds-text-title_caps">
                        <th>
                            <div class="slds-truncate slds-text-align_center" title=""></div>
                        </th>
                        <th>
                            <div class="slds-truncate slds-text-align_center" title="Target">Target</div>
                        </th>
                        <th>
                            <div class="slds-truncate slds-text-align_center" title="Max Point">Max Point</div>
                        </th>
                        <th>
                            <div class="slds-truncate slds-text-align_center" title="achievement">achievement</div>
                        </th>
                        <th>
                            <div class="slds-truncate slds-text-align_center" title="Points">Points</div>
                        </th>
                    </tr>
                </thead>
                
                <tbody>
                    <tr class="slds-hint-parent">
                        <td class="slds-cell-buffer_left slds-cell-buffer_right">
                            <div class="slds-text-align_left" style="color: rgb(0, 112, 210);"></div>
                        </td>
                        <td class="slds-cell-buffer_left slds-cell-buffer_right">
                            <div class="slds-text-align_left" style="color: rgb(0, 112, 210);">Project Completion</div>
                        </td>
                        <td class="slds-cell-buffer_left slds-cell-buffer_right">
                            <div class="slds-text-align_left" style="color: rgb(0, 112, 210);">Project Completion</div>
                        </td>
                        <td class="slds-cell-buffer_left slds-cell-buffer_right">
                            <div class="slds-text-align_left" style="color: rgb(0, 112, 210);">Customer handling</div>
                        </td>
                        <td class="slds-cell-buffer_left slds-cell-buffer_right">
                            <div class="slds-text-align_left" style="color: rgb(0, 112, 210);">Customer handling</div>
                        </td>
                    </tr>
                </tbody>
            </table>
            
        </lightning:accordionSection>
